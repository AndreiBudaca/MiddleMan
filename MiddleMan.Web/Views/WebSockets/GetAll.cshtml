@model List<MiddleMan.Web.Controllers.WebSockets.Model.WebSocketClientModel>

<div style="text-align: left">
  <h3>Your active clients</h3>
  @if (Model.Count != 0)
  {
    @foreach (var client in Model)
    {
      @await Html.PartialAsync("ClientCard", client!)
    }
  }
  else
  {
    <p>No active clients</p>
  }
</div>

<script src="/js/app/WebSockets/ClientCard.js" type="module" asp-append-version="true"></script>